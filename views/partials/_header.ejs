<header>
    <% if (typeof user !== 'undefined') { %>
    <p><form action="/logout?_method=DELETE" method="post">Logged in as <%= user.email %>. <input type="submit" value="Logout" class="btn btn-outline-info btn-sm"></form></p>
    <% } else { %>
    <a href="/login">Login</a> | <a href="/register">Register</a>
    <% } %>
    <br /><br />
</header>